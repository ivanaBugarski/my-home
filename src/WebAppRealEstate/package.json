{
  "name": "react-boilerplate",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@chakra-ui/icons": "2.0.8",
    "@chakra-ui/react": "^2.6.0",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@stripe/react-stripe-js": "^2.1.1",
    "@stripe/stripe-js": "^1.54.1",
    "@syncfusion/ej2-react-maps": "^21.2.10",
    "@tanstack/react-query": "4.0.10",
    "@tanstack/react-table": "8.5.11",
    "aws-sdk": "^2.1403.0",
    "axios": "0.27.2",
    "chakra-react-select": "4.1.4",
    "crypto": "^1.0.1",
    "crypto-browserify": "^3.12.0",
    "date-fns": "2.29.1",
    "firebase": "9.13.0",
    "formik": "2.2.9",
    "framer-motion": "^7.6.5",
    "i18next": "21.8.14",
    "leaflet-control-geocoder": "^2.4.0",
    "ol": "^7.4.0",
    "react": "18.2.0",
    "react-big-calendar": "1.6.3",
    "react-calendar-timeline": "0.28.0",
    "react-datepicker": "4.8.0",
    "react-dnd": "16.0.1",
    "react-dnd-html5-backend": "16.0.1",
    "react-dom": "18.2.0",
    "react-firebase-hooks": "5.0.3",
    "react-i18next": "11.18.1",
    "react-leaflet": "^4.2.1",
    "react-router-dom": "6.3.0",
    "react-s3": "^1.3.1",
    "react-select": "5.4.0",
    "rrule": "2.7.2",
    "rxjs": "^7.8.1",
    "uuid": "^9.0.0",
    "web-vitals": "2.1.4",
    "yup": "0.32.11"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js",
    "lint": "eslint .",
    "lint:fix": "eslint --fix",
    "format": "prettier --write \"./**/*.{js,jsx,ts,tsx,css,md,json, geojson}\" --config ./.prettierrc"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "7.16.0",
    "@pmmmwh/react-refresh-webpack-plugin": "0.5.3",
    "@svgr/webpack": "6.3.1",
    "@testing-library/jest-dom": "5.16.4",
    "@testing-library/react": "13.3.0",
    "@testing-library/user-event": "14.3.0",
    "@types/jest": "28.1.6",
    "@types/leaflet": "^1.9.3",
    "@types/node": "18.6.1",
    "@types/react": "18.0.15",
    "@types/react-big-calendar": "1.6.0",
    "@types/react-calendar-timeline": "0.26.7",
    "@types/react-datepicker": "4.8.0",
    "@types/react-dom": "18.0.6",
    "@typescript-eslint/eslint-plugin": "5.31.0",
    "@typescript-eslint/parser": "5.31.0",
    "babel-jest": "27.4.2",
    "babel-loader": "8.2.3",
    "babel-plugin-named-asset-import": "0.3.8",
    "babel-preset-react-app": "10.0.1",
    "bfj": "7.0.2",
    "browserslist": "4.18.1",
    "camelcase": "6.2.1",
    "case-sensitive-paths-webpack-plugin": "2.4.0",
    "css-loader": "6.5.1",
    "css-minimizer-webpack-plugin": "3.2.0",
    "dotenv": "10.0.0",
    "dotenv-expand": "5.1.0",
    "eslint": "8.20.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-react-app": "7.0.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.30.1",
    "eslint-webpack-plugin": "3.1.1",
    "file-loader": "6.2.0",
    "fs-extra": "10.0.0",
    "html-webpack-plugin": "5.5.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "27.4.3",
    "jest-resolve": "27.4.2",
    "jest-watch-typeahead": "1.0.0",
    "mini-css-extract-plugin": "2.4.5",
    "postcss": "8.4.4",
    "postcss-flexbugs-fixes": "5.0.2",
    "postcss-loader": "6.2.1",
    "postcss-normalize": "10.0.1",
    "postcss-preset-env": "7.0.1",
    "prettier": "2.7.1",
    "prompts": "2.4.2",
    "react-app-polyfill": "3.0.0",
    "react-dev-utils": "12.0.1",
    "react-refresh": "0.11.0",
    "resolve": "1.20.0",
    "resolve-url-loader": "4.0.0",
    "sass-loader": "12.3.0",
    "semver": "7.3.5",
    "source-map-loader": "3.0.0",
    "style-loader": "3.3.1",
    "terser-webpack-plugin": "5.2.5",
    "tsconfig-paths-webpack-plugin": "3.5.2",
    "typescript": "4.9.5",
    "webpack": "5.64.4",
    "webpack-dev-server": "4.9.3",
    "webpack-manifest-plugin": "4.0.2",
    "workbox-webpack-plugin": "6.5.4"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.ts"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.(js|jsx|mjs|cjs|ts|tsx)$": "<rootDir>/config/jest/babelTransform.js",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|cjs|ts|tsx|css|json|geojson)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs|cjs|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy",
      "^src/(.*)$": "<rootDir>/src/$1"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "geojson",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ],
    "resetMocks": true
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  }
}
